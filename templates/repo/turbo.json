{
	"$schema": "https://turborepo.org/schema.json",
	"ui": "tui",
	"tasks": {
		"topo": {
			"dependsOn": [
				"^topo"
			]
		},
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				".cache/tsbuildinfo.json",
				"dist/**",
				".react-email/**"
			]
		},
		"dev": {
			"dependsOn": [
				"^dev"
			],
			"cache": false,
			"persistent": false
		},
		"dev:ssl": {
			"dependsOn": [
				"^dev:ssl"
			],
			"cache": false,
			"persistent": false
		},
		"format": {
			"outputs": [
				".cache/.format"
			],
			"outputLogs": "new-only"
		},
		"lint": {
			"dependsOn": [
				"^topo",
				"^build"
			],
			"outputs": [
				".cache/.lint"
			]
		},
		"typecheck": {
			"dependsOn": [
				"^topo",
				"^build"
			],
			"outputs": [
				".cache/tsbuildinfo.json"
			]
		},
		"clean": {
			"cache": false
		},
		"//#clean": {
			"cache": false
		},
		"push": {
			"cache": false,
			"interactive": true
		},
		"studio": {
			"cache": false,
			"persistent": true
		},
		"ui-add": {
			"cache": false,
			"interactive": true
		}
	},
	"globalEnv": [
		"DATABASE_URL"
	],
	"globalPassThroughEnv": [
		"*_ENV",
		"CI",
		"VERCEL_*"
	]
}