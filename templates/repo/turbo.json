{
	"$schema": "https://turborepo.org/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				".cache/tsbuildinfo.json",
				".react-email/**",
				"dist/**"
			]
		},
		"dev": {
			"dependsOn": [
				"^dev"
			],
			"cache": false,
			"persistent": false
		},
		"test": {},
		"format": {
			"outputs": [
				".cache/.format"
			],
			"outputLogs": "new-only"
		},
		"lint": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				".cache/.lint"
			]
		},
		"typecheck": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				".cache/tsbuildinfo.json"
			]
		},
		"clean": {
			"cache": false
		}
	},
	"globalEnv": [],
	"globalPassThroughEnv": [
		"*_ENV",
		"CI",
		"VERCEL_*"
	]
}