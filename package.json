{
  "name": "startupkit",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "pnpm --stream -r run clean",
    "build": "pnpm --stream -r --filter \"./packages/**\" run build",
    "build:watch": "pnpm --stream -r run build:watch",
    "dev": "pnpm --stream -r run dev",
    "lint": "turbo run lint --continue --ui stream",
    "lint:fix": "turbo run lint:fix --continue --ui stream",
    "typecheck": "turbo run typecheck --ui stream",
    "bump": "./scripts/bump",
    "oldrelease": "pnpm --stream --filter \"./packages/**\" exec -- pnpm release --no-git-checks --access public",
    "release": "pnpm build && pnpm -r publish --access public --no-git-checks",
    "agents.md": "ruler apply",
    "link:local": "node scripts/link-local.js link",
    "unlink:local": "node scripts/link-local.js unlink"
  },
  "packageManager": "pnpm@9.6.0",
  "author": "Ian Hunter <ian@01.studio>",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@intellectronica/ruler": "^0.3.11",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-swc": "^0.3.1",
    "@swc/core": "^1.7.6",
    "glob": "^11.0.3",
    "rollup": "^4.20.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-preserve-directives": "^1.1.1",
    "tsup": "^8.2.4",
    "turbo": "2.5.0",
    "typescript": "5.9.2",
    "validate-package-exports": "^0.6.0"
  }
}